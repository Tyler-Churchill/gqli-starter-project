{
  "name": "gqli-starter-project",
  "version": "1.0.0",
  "description": "Instantly create a GraphQL server. Out of the box opinionated code modularization with Apollo Server under the hood. Skip the boilerplate you were going to do anyways!",
  "main": "index.js",
  "scripts": {
    "precommit": "pretty-quick --staged",
    "build": "babel source -d dist --copy-files",
    "start": "npm run-script build && node dist/index.js"
  },
  "dependencies": {
    "gqli": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-import-graphql": "^2.5.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.1.0",
    "prettier": "^1.13.5"
  },
  "author": "Tyler Churchill",
  "license": "MIT"
}
